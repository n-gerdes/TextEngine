function initialize()
	set_global_value("rooms_generated","1");
	set_global_variable("open_halls", 4);
	set_global_variable("room_0_0_shape", "RNESW");
	set_global_variable("generated_exit",false);
	scene(room).clone("room_0_0");
	entity(~player).set_var("x", 0);
	entity(~player).set_var("y", 0);
	entity(~player).transfer(room_0_0);
end

